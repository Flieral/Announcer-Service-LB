{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": true
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Email": {
    "dataSource": "emailDS"
  },
  "client": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "campaign": {
    "dataSource": "mongoDs",
    "public": true
  },
  "subcampaign": {
    "dataSource": "mongoDs",
    "public": true
  },
  "setting": {
    "dataSource": "mongoDs",
    "public": true
  },
  "announcerAccount": {
    "dataSource": "mongoDs",
    "public": true
  },
  "container": {
    "dataSource": "storage",
    "public": true
  }
}
